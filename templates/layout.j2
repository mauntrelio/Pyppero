<!doctype html>
<html lang="{{ _('en') }}">
<head>
	{% block head %}
	<meta charset="UTF-8">
	<title>{% block title %}{% if page.title %}{{ page.title }} &mdash; {% endif %}{{ domain_name }}{% endblock %}</title>
	<link rel="icon" href="/assets/img/favicon.ico?v=20140210" />
	<link type="text/css" rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300,600|Open+Sans+Condensed:300,600&amp;v2">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">
	<link href="/assets/css/style.css" rel="stylesheet" type="text/css">
	{% endblock %}
</head>
<body>
	{% block body %}
	{% endblock %}
	<script>
	  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

	  ga('create', 'UA-4275314-7', '{{ domain_name }}');
	  ga('send', 'pageview');
	</script>
	
</body>
</html>